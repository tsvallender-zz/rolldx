<%= form_with(model: @list) do |f| %>

<%= render 'shared/error_messages', object: f.object %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :private %>
<%= f.check_box :private %>

<% if (defined? table_ids) && table_ids.present? %>
  <% table_ids.each do |id| %>
  <%= f.hidden_field :table_ids, multiple: true, value: id %>
  <% end %>
<% end %>
<%= f.submit yield(:button_text) %>

<% end %>
