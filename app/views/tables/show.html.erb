<% if @table.user == current_user %>
<%= link_to 'Edit table', edit_table_path(@table) %>
<% end %>
<%= render @table %>

<%= form_with(model: @listmember) do |f| %>
<%= f.hidden_field :table_id, value: @table.id %>
<%= f.select :list, current_user.lists %>
<%= f.submit "Add table to list" %>
<% end %>
